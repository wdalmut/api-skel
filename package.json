    "body-parser": "^1.19.0",
    "config": "^1.31.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-joi-validation": "^0.2.1",
    "express-pino-logger": "^4.0.0",
    "install": "^0.10.4",
    "joi": "^13.7.0",
